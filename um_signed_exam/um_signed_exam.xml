<?xml version="1.0"?>
<openerp>
	<data>
        <record id="view_form_um_signed_exam" model="ir.ui.view">
            <field name="name">Signed Exam Tree</field>
			<field name="model">um.signed.exam</field>
			<field name="arch" type="xml">
                <tree string="Exam">
                    <!--<field name="name" />-->
					<field name="subject_id" />
					<field name="start_time" />
					<field name="end_time" />
					<field name="note" />
					<field name="student_id"/>
				</tree>
            </field>
        </record>

        <act_window id="action_um_signed_exam"
                    name="Signed Exam"
                    res_model="um.signed.exam"
                    view_mode="tree"/>
    </data>
</openerp>